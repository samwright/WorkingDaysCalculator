<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<fx:root type="javafx.scene.layout.AnchorPane" id="AnchorPane" maxHeight="300.0" maxWidth="300.0"
         xmlns:fx="http://javafx.com/fxml">
  <children>
    <GridPane prefHeight="414.0" prefWidth="611.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <HBox id="HBox" alignment="CENTER" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
          <children>
            <Label text="Duration" />
          </children>
        </HBox>
        <HBox id="HBox" alignment="CENTER" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
          <children>
            <Label text="Start Date" />
          </children>
        </HBox>
        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
          <children>
            <VBox id="VBox" alignment="CENTER" spacing="5.0">
              <children>
                <GridPane minHeight="-Infinity">
                  <children>
                    <ChoiceBox fx:id="dayChoiceBox" GridPane.columnIndex="0"
                               GridPane.rowIndex="1"/>
                    <ChoiceBox fx:id="monthChoiceBox" GridPane.columnIndex="1"
                               GridPane.rowIndex="1"/>
                    <HBox id="HBox" alignment="CENTER" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
                      <children>
                        <Label text="Day" />
                      </children>
                    </HBox>
                    <HBox id="HBox" alignment="CENTER" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
                      <children>
                        <Label text="Month" />
                      </children>
                    </HBox>
                    <HBox id="HBox" alignment="CENTER" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="0">
                      <children>
                        <Label text="Year" />
                      </children>
                    </HBox>
                    <ChoiceBox fx:id="yearChoiceBox" GridPane.columnIndex="2"
                               GridPane.rowIndex="1"/>
                  </children>
                  <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  </columnConstraints>
                  <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  </rowConstraints>
                </GridPane>
              </children>
            </VBox>
          </children>
        </HBox>
        <HBox id="HBox" alignment="CENTER" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="2">
          <children>
            <Label text="End Date" />
          </children>
        </HBox>
        <HBox id="HBox" alignment="CENTER" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
          <children>
            <TextField fx:id="durationTextField" prefWidth="200.0" />
            <Label minHeight="-Infinity" minWidth="-Infinity" text="working days">
              <HBox.margin>
                <Insets />
              </HBox.margin>
            </Label>
          </children>
        </HBox>
        <HBox id="HBox" alignment="CENTER" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
          <children>
            <Label fx:id="endDateLabel" text="Uncalculated" />
          </children>
        </HBox>
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="NEVER" maxWidth="148.0" minWidth="10.0" prefWidth="83.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="217.0" minWidth="10.0" prefWidth="217.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
  </children>
</fx:root>
